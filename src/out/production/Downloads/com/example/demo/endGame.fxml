      </Label>
      <Button layoutX="336.0" layoutY="418.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="108.0" text="Menu" onAction="#returnMenu"/>
      <Label fx:id="curScore" layoutX="228.0" layoutY="488.0" textFill="WHITE">
